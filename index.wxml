<!--index.wxml-->
<button type="primary" style="margin-bootom:20px;margin-top:30px" bindtap="getToken">申请令牌</button>
<button type="primary" style="margin-bootom:20px;margin-top:30px" bindtap="getSuperToken">申请super令牌</button>
<button type="warn" style="margin-bootom:20px;margin-top:30px" bindtap="pay">支付</button>
<button type="primary" style="margin-bootom:20px;margin-top:30px" bindtap="delivery">发货</button>
<form bindsubmit="formID" report_submit="true">
<button form-type="submit" type="primary" style="margin-bootom:20px;margin-top:30px">表单ID</button>
</form>
<button type="warn" bindtap="checkSession">检查登陆状态</button>
